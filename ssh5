case "$x" in 
   1 | 01)
   clear
rm Plus
rm /usr/bin/ssh5
   SSL
   exit;
   ;;
    2 | 02)
   clear
rm Plus
rm /usr/bin/ssh5
   senharoot
   exit;
   ;;
    3 | 03)
   clear
rm Plus
rm /usr/bin/ssh5
   tcp
   exit;
