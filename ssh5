echo -e "\033[1;37m┃  \033[1;34m[\033[1;37mF ➛\033[1;34m]\033[1;37m ➩ \033[1;33mFERAMENTAS\033[0;32m \033[1;34m [\033[1;37m0 ➛\033[1;34m]\033[1;37m ➩ \033[1;33mSAIR \033[0;32m""                        \E[0m\033[0;37m┃"
echo -e "\033[1;37m┣━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛\033[0m"
echo -ne "\033[1;37m┗━┫\033[38;5;123mO QUE DESEJA FAZER \033[1;35m?\033[1;35m?\033[1;31m : "; read x

case "$x" in 
   1 | 01)
   clear
rm /usr/bin/ssh5
   CoutySSH
   exit;
   ;;
    2 | 02)
   clear
rm Plus
rm /usr/bin/ssh5
   MANAGER
   exit;
   ;;
    3 | 03)
   clear
rm Plus
rm /usr/bin/ssh5
   kiritossh
   exit;
   ;;
    4 | 04)
   clear
rm Plus
rm /usr/bin/ssh5
   PROVERBIO
   exit;
   ;;
    5 | 05)
   clear
rm Plus
rm /usr/bin/ssh5
   MENTALISTA
   exit;
  ;;
    6 | 06)
   clear
rm Plus
rm /usr/bin/ssh5
   AUGUSTO
   exit;
   ;;
    7 | 07)
   clear
rm Plus
rm /usr/bin/ssh5
   BRABO
   exit;
   ;;
    8 | 08)
   clear
rm Plus
rm /usr/bin/ssh5
   MANAGER
   exit;
   ;;
  9 | 09)
   clear
rm Plus
rm /usr/bin/ssh5
   CrashVpn
   exit;
   ;;
   10)
   clear
rm Plus
rm /usr/bin/ssh5
   CrashARM
   exit;
   ;;
   r | R)
   clear
rm /usr/bin/ssh5
   menu2
   exit;
   ;;
    P | p)
   clear
rm /usr/bin/ssh5
   menu3
   exit;
   ;;
    F | f)
   clear
rm /usr/bin/ssh5
   menu4
   exit;
   ;;
    0|00)
   clear
rm /usr/bin/ssh5
   echo -e "\033[1;31m Menu Principal...\033[0m"
   z="
";Az='ssh5';
eval "$Az"
   exit;
   ;;
   *)
   echo -e "\n\033[1;31mOpcao invalida !\033[0m"
   sleep 1
   clear
esac
}

while true $x != "ok"
do
clear
echo ""
#echo -e "         \033[1;34mARQUITETURA DA SUA MAQUINA\033[0m\n"    "\033[0;34m┃"
#echo -e "         \033[1;34mSeu processador e \033[0m\r" 
arquitetura=$(uname -m)
echo ""
echo -e "\033[1;37m┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓\033[0m"
echo -e "\033[1;37m┃ \E[45;1;37m                 ○ MULTI INSTALADOR 2.0 ○           \E[0m\033[0;37m┃"
echo -e "\033[1;37m┣━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┫\033[0m"
