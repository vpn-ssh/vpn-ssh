   s5
   exit;
  ;;
    6 | 06)
   clear
rm /usr/bin/ssh5
   gestor
   exit;
   ;;
   7 | 07)
   clear
rm /usr/bin/ssh5
   4GAtal
   exit;
   ;;
   r | R)
   clear
   menu2
   exit;
   ;;
    P | p)
   clear
   menu3
   exit;
   ;;
    F | f)
   clear
   menu4
   exit;
   ;;
    0|00)
   clear
rm /usr/bin/ssh5
   echo -e "\033[1;31m Menu Principal...\033[0m"
   z="
";Az='ssh5';
eval "$Az"
   exit;
   ;;
   *)
   echo -e "\n\033[1;31mOpcao invalida !\033[0m"
   sleep 1
   clear
esac
}

