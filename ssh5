echo -e "\033[1;37m┃ " "SITE: https://coutyssh.online/ para mais detalhes " "\E[0m\033[0;37m┃"
echo -e "\033[1;37m┣━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┫\033[0m"
